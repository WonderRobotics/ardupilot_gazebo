<?xml version="1.0"?>

<model>
  <name>Atalef</name>
  <version>2.0</version>
  <sdf version="1.9">model.sdf</sdf>

  <author>
    <name>Fadri Furrer</name>
    <email>fadri.furrer@mavt.ethz.ch</email>
  </author>
  <author>
    <name>Michael Burri</name>
  </author>
  <author>
    <name>Mina Kamel</name>
  </author>
  <author>
    <name>Janosch Nikolic</name>
  </author>
  <author>
    <name>Markus Achtelik</name>
  </author>

  <author>
    <name>Rhys Mainwaring</name>
  </author>

  <description>
    A model for an Atalef drone with
    a LIDAR facing downwards,
    a down facing camera
    and a front facing camera.
  </description>
  <depend>
    <model>
      <uri>model://iris_with_standoffs</uri>
      <version>2.0</version>
    </model>
    <model>
      <uri>model://fixed_camera</uri>
      <version>2.0</version>
    </model>
    <model>
      <uri>model://lidar_down</uri>
      <version>2.0</version>
    </model>
  </depend>
</model>
